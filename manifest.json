{
  "manifest_version": 3,
  "name": "Streak Auto v2",
  "version": "1.0.0",
  "description": "Making auto trading easy!",
  "icons": {
    "16": "images/get_started16.png",
    "48": "images/get_started16.png",
    "128": "images/get_started16.png"
  },
  "background": {
    "service_worker": "background-b.js"
  },
  "content_scripts": [
    {
      "js": ["jquery.js", "contentScript.js"],
      "matches": [
        "*://*.zerodha.com/*",
        "*://*.streak.tech/*",
        "*://*.streak.ninja/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "declarativeNetRequest",
    "scripting",
    "storage",
    "notifications",
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "*://api-op.streak.tech/*",
    "*://streak.zerodha.com/*",
    "*://streakv3.zerodha.com/*",
    "*://*.streak.tech/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/get_started16.png"
  },
  "options_page": "options.html",
  "update_url": "https://clients2.google.com/service/update2/crx"
}
