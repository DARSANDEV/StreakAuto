{
  "background": {
     "persistent": true,
     "scripts": [ "jquery.js", "moment.js", "background.js" ]
  },
  "content_scripts": [ {
     "js": [ "jquery.js", "contentScript.js" ],
     "matches": [ "*://*.zerodha.com/*","*://*.streak.tech/*", "*://*.streak.ninja/*" ],
     "run_at": "document_start"
  } ],
  "description": "Making auto trading easy!",
  "icons": {
     "128": "images/get_started16.png",
     "16": "images/get_started16.png",
     "48": "images/get_started16.png"
  },
  "manifest_version": 2,
  "name": "Streak Auto v2",
  "options_page": "options.html",
  "page_action": {
     "default_icon": {
        "16": "images/get_started16.png"
     },
     "default_popup": "popup.html"
  },
  "permissions": [ "webRequest", "notifications", "storage", "activeTab", "declarativeContent", "webRequest", "*://*.zerodha.com/","*://*.streak.tech/*", "*://*.streak.ninja/*" ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}